  <div class="col-lg-3">
    <%= link_to service_show_path(service) do%>
      <div class="card" data-id="<%= service.id %>">
        <div><%= image_tag(service.thumb_img, class: "index-image") unless service.thumb_img.nil?  %></div>
        <div class="card-text">
          <h3><%=service.title%></h3>
        </div>
      </div>
    <% end %>
  </div>