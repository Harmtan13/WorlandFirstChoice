<% @contact.each do |info| %>


  <div class="card">
    <div class="card-body">
    <h2 class="title"> <%= info.title %> </h2>
      <div class="row card-text">
        <div class="col-md-6">
          <h4>Phone-Number</h4>
            <p><%= number_to_phone(info.phone_number, area_code: true) %></p>
          <h4>Fax-Number</h4>
            <p><%= number_to_phone(info.fax_number, area_code: true) %></p>
          <h4>Email</h4>
            <p><a href="mailto:<%= info.email %>"><%= info.email %></a></p>
          <h4>Address</h4>
              <p><%= info.address %></p>
          <%= link_to "Facebook", "https://www.facebook.com/Worland-Physical-Therapy-1551997184864218/", :target => "_blank" %>
        </div>

        <div class="col-md-6">
          <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2869.2590022562595!2d-107.94832891978803!3d44.01604203701151!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x534b6d5c042ca119%3A0x537e2300155423f9!2s120+S+17th+St%2C+Worland%2C+WY+82401!5e0!3m2!1sen!2sus!4v1527871429889" width="300" height="300" frameborder="0" style="border:0" allowfullscreen></iframe>

          <div><%= link_to "Edit", edit_contact_path(info.id) if logged_in?(:site_admin, :editor)%></div>
        </div>
      </div>
  </div>
</div>
<% end %>