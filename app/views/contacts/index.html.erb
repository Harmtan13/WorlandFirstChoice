<% @contact.each do |info| %>
  <h1> <%= info.title %> </h1>
  <p> <%= number_to_phone(info.phone_number, area_code: true) %> </p>
  <p> <%= number_to_phone(info.fax_number, area_code: true) %> </p>
  <p> <%= info.email %> </p>
  <p> <%= info.address %> </p>
  <div><%= image_tag info.main_img %></div>
  <div><%= link_to "Edit", edit_contact_path(info.id) %></div>
<% end %>